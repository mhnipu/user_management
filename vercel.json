  "buildCommand": "npm run build",
  "outputDirectory": "dist/user-management-app/browser",
  "framework": "angular",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "dist/user-management-app/server/server.mjs": {
      "maxDuration": 30
    }
  }
}
